<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CREATED_BY" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_CREATED_BY" hidden="true" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_MODIFIED_BY" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_MODIFIED_BY" hidden="true" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_MODIFIED_BY"/>
    <field name="user_id" default="0" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_USER_ID" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_USER_ID" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_USER_ID"/>
    <field name="seen_flag" default="0" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_SEEN_FLAG" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_SEEN_FLAG" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_SEEN_FLAG"/>
    <field name="seen_at" default="0000-00-00 00:00:00" type="calendar" class="inputbox" format="%Y-%m-%d %H:%M:%S" showtime="true" translateformat="true" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_SEEN_AT" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_SEEN_AT" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_SEEN_AT"/>
    <field name="title" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_TITLE" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_TITLE" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_TITLE"/>
    <field name="message" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_MESSAGE" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_MESSAGE" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_MESSAGE"/>
    <field name="created_date" type="timecreated" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CREATED_DATE" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_CREATED_DATE" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CREATED_DATE"/>
    <field name="updated_date" type="timecreated" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_UPDATED_DATE" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_UPDATED_DATE" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_UPDATED_DATE"/>
    <field name="category" extension="com_notifications_user.notificationusers" type="category" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CATEGORY" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_CATEGORY" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_CATEGORY"/>
    <field name="status" default="1" type="list" label="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_STATUS" description="COM_NOTIFICATIONS_USER_FORM_DESC_NOTIFICATIONUSER_STATUS" hint="COM_NOTIFICATIONS_USER_FORM_LBL_NOTIFICATIONUSER_STATUS">
      <option value="1">COM_NOTIFICATIONS_USER_NOTIFICATIONUSERS_STATUS_OPTION_1</option>
      <option value="2">COM_NOTIFICATIONS_USER_NOTIFICATIONUSERS_STATUS_OPTION_2</option>
      <option value="99">COM_NOTIFICATIONS_USER_NOTIFICATIONUSERS_STATUS_OPTION_99</option>
    </field>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
