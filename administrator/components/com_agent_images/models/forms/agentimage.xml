    <field name="image1" type="FileMultiple" label="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE1" description="COM_AGENT_IMAGES_FORM_DESC_AGENTIMAGE_IMAGE1" hint="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE1"/>
    <field name="image2" type="FileMultiple" label="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE2" description="COM_AGENT_IMAGES_FORM_DESC_AGENTIMAGE_IMAGE2" hint="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE2"/>
