    <field name="image4" type="FileMultiple" label="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE4" description="COM_AGENT_IMAGES_FORM_DESC_AGENTIMAGE_IMAGE4" hint="COM_AGENT_IMAGES_FORM_LBL_AGENTIMAGE_IMAGE4"/>
