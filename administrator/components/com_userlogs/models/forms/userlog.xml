<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_CREATED_BY" description="COM_USERLOGS_FORM_DESC_USERLOG_CREATED_BY" hidden="true" hint="COM_USERLOGS_FORM_LBL_USERLOG_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_MODIFIED_BY" description="COM_USERLOGS_FORM_DESC_USERLOG_MODIFIED_BY" hidden="true" hint="COM_USERLOGS_FORM_LBL_USERLOG_MODIFIED_BY"/>
    <field name="status" default="1" type="list" label="COM_USERLOGS_FORM_LBL_USERLOG_STATUS" description="COM_USERLOGS_FORM_DESC_USERLOG_STATUS" hint="COM_USERLOGS_FORM_LBL_USERLOG_STATUS">
      <option value="1">COM_USERLOGS_USERLOGS_STATUS_OPTION_1</option>
      <option value="2">COM_USERLOGS_USERLOGS_STATUS_OPTION_2</option>
      <option value="9">COM_USERLOGS_USERLOGS_STATUS_OPTION_9</option>
    </field>
    <field name="agent_id" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_AGENT_ID" description="COM_USERLOGS_FORM_DESC_USERLOG_AGENT_ID" hint="COM_USERLOGS_FORM_LBL_USERLOG_AGENT_ID"/>
    <field name="user_id" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_USER_ID" description="COM_USERLOGS_FORM_DESC_USERLOG_USER_ID" hint="COM_USERLOGS_FORM_LBL_USERLOG_USER_ID"/>
    <field name="old_level" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_OLD_LEVEL" description="COM_USERLOGS_FORM_DESC_USERLOG_OLD_LEVEL" hint="COM_USERLOGS_FORM_LBL_USERLOG_OLD_LEVEL"/>
    <field name="new_level" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_NEW_LEVEL" description="COM_USERLOGS_FORM_DESC_USERLOG_NEW_LEVEL" hint="COM_USERLOGS_FORM_LBL_USERLOG_NEW_LEVEL"/>
    <field name="created_date" default="0000-00-00 00:00:00" type="calendar" class="inputbox" format="%Y-%m-%d %H:%M:%S" showtime="true" translateformat="true" label="COM_USERLOGS_FORM_LBL_USERLOG_CREATED_DATE" description="COM_USERLOGS_FORM_DESC_USERLOG_CREATED_DATE" hint="COM_USERLOGS_FORM_LBL_USERLOG_CREATED_DATE"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="type" extension="com_userlogs.userlogs" type="category" label="COM_USERLOGS_FORM_LBL_USERLOG_TYPE" description="COM_USERLOGS_FORM_DESC_USERLOG_TYPE" hint="COM_USERLOGS_FORM_LBL_USERLOG_TYPE"/>
    <field name="transfer_id" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_TRANSFER_ID" description="COM_USERLOGS_FORM_DESC_USERLOG_TRANSFER_ID" hint="COM_USERLOGS_FORM_LBL_USERLOG_TRANSFER_ID"/>
    <field name="customer_id" default="0" label="COM_USERLOGS_FORM_LBL_USERLOG_CUSTOMER_ID" description="COM_USERLOGS_FORM_DESC_USERLOG_CUSTOMER_ID" hint="COM_USERLOGS_FORM_LBL_USERLOG_CUSTOMER_ID"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
