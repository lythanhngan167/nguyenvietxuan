<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_BOOK_FORM_LBL_BOOKING_CREATED_BY" description="COM_BOOK_FORM_DESC_BOOKING_CREATED_BY" hidden="true" hint="COM_BOOK_FORM_LBL_BOOKING_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_BOOK_FORM_LBL_BOOKING_MODIFIED_BY" description="COM_BOOK_FORM_DESC_BOOKING_MODIFIED_BY" hidden="true" hint="COM_BOOK_FORM_LBL_BOOKING_MODIFIED_BY"/>
    <field name="phone" required="true" label="COM_BOOK_FORM_LBL_BOOKING_PHONE" description="COM_BOOK_FORM_DESC_BOOKING_PHONE" hint="COM_BOOK_FORM_LBL_BOOKING_PHONE"/>
    <field name="address" default="1" type="list" required="true" label="COM_BOOK_FORM_LBL_BOOKING_ADDRESS" description="COM_BOOK_FORM_DESC_BOOKING_ADDRESS" hint="COM_BOOK_FORM_LBL_BOOKING_ADDRESS">
      <option value="1">COM_BOOK_BOOKINGS_ADDRESS_OPTION_1</option>
    </field>
    <field name="booking_date" class="inputbox" format="%d-%m-%Y" timeformat="24" filter="server_utc" translateformat="false" showtime="false" singleheader="false" todaybutton="false" weeknumbers="false" filltable="false" type="calendar" required="true" label="COM_BOOK_FORM_LBL_BOOKING_BOOKING_DATE" description="COM_BOOK_FORM_DESC_BOOKING_BOOKING_DATE" hint="COM_BOOK_FORM_LBL_BOOKING_BOOKING_DATE"/>
    <field name="hours" type="combo" required="true" label="COM_BOOK_FORM_LBL_BOOKING_HOURS" description="COM_BOOK_FORM_DESC_BOOKING_HOURS" hint="COM_BOOK_FORM_LBL_BOOKING_HOURS">
      <option value="9">COM_BOOK_BOOKINGS_HOURS_OPTION_9</option>
      <option value="10">COM_BOOK_BOOKINGS_HOURS_OPTION_10</option>
      <option value="11">COM_BOOK_BOOKINGS_HOURS_OPTION_11</option>
      <option value="12">COM_BOOK_BOOKINGS_HOURS_OPTION_12</option>
      <option value="13">COM_BOOK_BOOKINGS_HOURS_OPTION_13</option>
      <option value="14">COM_BOOK_BOOKINGS_HOURS_OPTION_14</option>
      <option value="15">COM_BOOK_BOOKINGS_HOURS_OPTION_15</option>
      <option value="16">COM_BOOK_BOOKINGS_HOURS_OPTION_16</option>
      <option value="17">COM_BOOK_BOOKINGS_HOURS_OPTION_17</option>
      <option value="18">COM_BOOK_BOOKINGS_HOURS_OPTION_18</option>
      <option value="19">COM_BOOK_BOOKINGS_HOURS_OPTION_19</option>
      <option value="20">COM_BOOK_BOOKINGS_HOURS_OPTION_20</option>
    </field>
    <field name="note" filter="safehtml" type="textarea" label="COM_BOOK_FORM_LBL_BOOKING_NOTE" description="COM_BOOK_FORM_DESC_BOOKING_NOTE" hint="COM_BOOK_FORM_LBL_BOOKING_NOTE"/>
    <field name="created_time" type="timecreated" label="COM_BOOK_FORM_LBL_BOOKING_CREATED_TIME" description="COM_BOOK_FORM_DESC_BOOKING_CREATED_TIME" hint="COM_BOOK_FORM_LBL_BOOKING_CREATED_TIME"/>
    <field name="modified_time" type="timecreated" label="COM_BOOK_FORM_LBL_BOOKING_MODIFIED_TIME" description="COM_BOOK_FORM_DESC_BOOKING_MODIFIED_TIME" hint="COM_BOOK_FORM_LBL_BOOKING_MODIFIED_TIME"/>
    <field name="status_id" default="1" type="list" required="true" label="COM_BOOK_FORM_LBL_BOOKING_STATUS_ID" description="COM_BOOK_FORM_DESC_BOOKING_STATUS_ID" hint="COM_BOOK_FORM_LBL_BOOKING_STATUS_ID">
      <option value="1">COM_BOOK_BOOKINGS_STATUS_ID_OPTION_1</option>
      <option value="2">COM_BOOK_BOOKINGS_STATUS_ID_OPTION_2</option>
      <option value="3">COM_BOOK_BOOKINGS_STATUS_ID_OPTION_3</option>
    </field>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
